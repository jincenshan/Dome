/* pages/member/grade.wxss */
.main{
  width: 750rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.content{
  width: 690rpx;
  height: 280rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  position: relative;
}
.content_top{
  width: 610rpx;
  display: flex;
  height: 200rpx;
  flex-direction: row;
  justify-content: space-between;
  border-bottom: 1rpx solid #96c2fd;
}
.content_images{
  width: 690rpx;
  height: 280rpx;
  display: flex;
  position: absolute;
  z-index: -1;
}
.content_top_left{
  height: 200rpx;
  display: flex;
  flex-direction: column;
  justify-content: flex-end;
  color: white;
}
.content_bottom{
  width: 610rpx;
  height: 80rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  color: #96c2fd;
  font-size: 24rpx;
  line-height: 24rpx;
}