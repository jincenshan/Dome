/**index.wxss**/
 .main{
   width: 750rpx;
   display: flex;
   flex-direction: column;
   overflow: hidden;
   align-items: center;
}
.map_style{
  width: 750rpx;
  height: 100%;
  position: absolute;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
}
.button_go{
  width: 400rpx;
  height: 100rpx;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  color: white;
  font-size: 32rpx;
  line-height: 36rpx;
  background-color: #0080FF;
  border-radius: 50rpx;
  position: fixed;
  z-index: 10;
  bottom: 30rpx;
  font-weight: bold;
}
button::after {
  border: none;
  border-radius:0;
  width: none;
}
.code_images{
  width: 60rpx;
  height: 60rpx;
  margin-right: 20rpx;
}
.map_view{
  width:690rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  position: fixed;
  z-index: 20;
  bottom: 10rpx;
  left: 30rpx;
  background-color: white;
  border-radius: 20rpx;
}
.map_view_go{
  width:630rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  padding: 30rpx;
}
.top_map{
  width: 750rpx;
  height: 70rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  background-color: #FF773B;
  font-size: 28rpx;
  color: white;
  position: fixed;
  top: 0;
  left: 0;
}
.top_map_view{
  display: flex;
  flex-direction: row;
  align-items: center;
  font-size: 28rpx;
  color: white;
  line-height: 28rpx;
}
.top_message{
  width: 620rpx;
  height: 60rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
  font-size: 32rpx;
  line-height: 32rpx;
  color: white;
  background: rgba(0,0,0,0.50);
  border-radius: 30rpx;
  position: fixed;
  top: 30rpx;
  left: 65rpx;
}
.content_text{
  width: 630rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
}
.map_view_text{
  width: 70%;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  font-size: 34rpx;
  line-height: 34rpx;
}
.map_view_state{
  width: 100%;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
}
.map_view_state_top{
  font-size: 24rpx;
  line-height: 24rpx;
  display: flex;
  flex-direction: row;
  color: #7d7d7d;
  margin-top: 24rpx;
}
.map_right_images{
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  align-items: center;
  font-size:28rpx;
  line-height: 36rpx;
}
.title_view{
  width: 750rpx;
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  position: fixed;
  z-index: 20;
}
.title_view_hread{
  width: 650rpx;
  height: 90rpx;
  font-size: 34rpx;
  line-height: 34rpx;
  border-top-left-radius: 30rpx;
  border-top-right-radius: 30rpx;
  background-color: white;
  border-bottom: 1rpx solid #e1e1e1;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
}
.title_view_content{
  width: 650rpx;
  height: 200rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  font-size: 30rpx;
  line-height: 56rpx;
  background-color: white;
  font-weight: 500;
}
.title_view_button{
  width: 650rpx;
  height: 90rpx;
  font-size: 28rpx;
  line-height: 28rpx;
  color:gray;
  border-bottom-left-radius: 30rpx;
  border-bottom-right-radius: 30rpx;
  border-top:1px solid#e1e1e1;
  background-color: white;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  background-color: white;
}
.title_button{
  width: 344rpx;
  height: 89rpx;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  font-size: 32rpx;
  line-height: 32rpx;
  border-bottom-left-radius: 30rpx;
  border-bottom-right-radius: 30rpx;
}

button::after{
  border:none;
  padding: none;
  border-radius: none;
}
button{
  border-radius:0;
  padding: 0;
  background: none;
  margin:0;
}
.line{
  width: 2rpx;
  height: 70rpx;
  background-color: #e1e1e1;
}
.map_views{
  width: 120rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  position: absolute;
  top: 70rpx;
  right: 0;
}
.map_view_images{
  width: 120rpx;
  height: 120rpx;
  margin-top: 20rpx;
}

/* 打电话 */
.call{
  width: 750rpx;
  height: 100%;
  position: absolute;
  z-index: 10;
  top: 0;
  left: 0;
  background: rgba(0, 0, 0, 0.3);
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}
.call_view{
  width: 690rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  position: relative;
  background-color: white;
  border-radius: 20rpx;
}
.call_images{
  width: 160rpx;
  height: 160rpx;
  position: absolute;
  top: -80rpx;
}
.call_bottom{
  width: 610rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  margin-bottom: 60rpx;
}
.call_bottom_button{
  width: 290rpx;
  height: 80rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
  font-size: 30rpx;
  line-height: 30rpx;
  font-weight: bold;
  border-radius: 40rpx;
}